{
  "name": "vue-tably",
  "version": "0.1.3",
  "main": "./dist/vue-tably.common.js",
  "description": "Dynamic Vue.js component to create tabs",
  "author": {
    "name": "Felipe Maia",
    "email": "webfelipemaia@gmail.com",
    "url": "https://github.com/webfelipemaia/vue-tably"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webfelipemaia/vue-tably.git"
  },
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:npm": "SET Node_ENV=production && vue-cli-service build --target lib --name vue-tably src/index.js && npm run scss",
    "postinstall": "node ./assets.js",
    "lint": "vue-cli-service lint",
    "scss": "node-sass --output-style compressed -o dist/ src/scss",
    "postcss": "postcss -u autoprefixer -r dist/css/*"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "html-webpack-plugin": "^4.4.1",
    "materialize-css": "^1.0.0-rc.2",
    "mini-css-extract-plugin": "^0.11.0",
    "postcss-cli": "^7.1.1",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.3",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "files": [
    "dist/*"
  ]
}
